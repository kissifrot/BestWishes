{% extends 'base.html.twig' %}

{% block title %}New category{% endblock %}

{% block content %}
    <div class="container">
        <ol class="breadcrumb">
            <li><a href="{{ path('list_show', { id: list.id } ) }}">{{ list.name }}</a></li>
            <li class="active">New category</li>
        </ol>

        <h1>New category</h1>
        {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.save) }}
        {{ form_end(form) }}
    </div>
{% endblock %}
